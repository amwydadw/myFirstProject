@import"./style/iconfont.wxss";
/* 在微信小程序不支持通配符”*“ */
page,view,text,swiper,swiper-item,image,navigtor{
margin: 0;
padding: 0;
box-sizing: border-box;
}
/* 主题色 通过变量来实现 
1 less中存在变量这个知识
2原生的css和wxss也是支持变量
*/
page{
  /* 定义主题色 */
  --themeColor:#1296db;
  /* 定义统一字体大小 假设设计稿大小是375px
  1px=2rpx
  14px=2rpx
  */
  --priceColor: #ff5000;
  font-size: 28rpx;
}
image{
  width: 100%;
}
