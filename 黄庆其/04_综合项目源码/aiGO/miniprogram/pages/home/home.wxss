.index_swiper swiper {
  width: 750rpx;
  height: 300rpx;
}
.index_swiper swiper swiper-item navigator {
  text-align: center;
  padding: 20rpx;
}
.index_swiper swiper swiper-item navigator image {
  width: 98%;
  border-radius: 20rpx;
  box-shadow: 4rpx 4rpx 5rpx #cccccc;
}
.home_nav .nav_top {
  display: flex;
}
.home_nav .nav_top .nav_item {
  padding: 20rpx 0;
  text-align: center;
  flex: 1;
}
.home_nav .nav_top .nav_item image {
  height: 120rpx;
}
.home_nav .nav_bottom {
  display: flex;
}
.home_nav .nav_bottom .nav_item {
  padding: 0 0 20rpx 0;
  text-align: center;
  flex: 1;
}
.home_nav .nav_bottom .nav_item image {
  height: 120rpx;
}
.index_floor {
  box-sizing: border-box;
}
.index_floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_list {
  overflow: hidden;
}
.index_floor .floor_group .floor_list navigator {
  float: left;
  width: 32.5%;
  /*控制后四个超链接图片*/
}
.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  /*原图的宽高232 *386 */
  height: 25.14116379vw;
  border-left: 10rpx solid #ffffff;
}
.index_floor .floor_group .floor_list navigator:nth-child(2),
.index_floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #ffffff;
}
.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
.commodity_box {
  overflow: hidden;
  margin: 20rpx 10rpx 0 10rpx;
  padding: 20rpx 10rpx 0 10rpx;
  border-radius: 20rpx;
  background-color: #f0f0f0;
  box-shadow: 4rpx 4rpx 5rpx #cccccc;
}
.commodity_box .commodity {
  float: left;
  width: 50%;
  padding: 10rpx 5rpx;
}
.commodity_box .commodity .commodityImage {
  border-radius: 20rpx;
}
.commodity_box .commodity .commodityImage image {
  width: 100%;
  border-radius: 20rpx;
}
.commodity_box .commodity .title {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.commodity_box .commodity .price_box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx;
}
.commodity_box .commodity .price_box .price {
  color: var(--priceColor);
}
.commodity_box .commodity .price_box .pay image {
  width: 30rpx;
}
